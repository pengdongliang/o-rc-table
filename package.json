{
  "name": "o-rc-table",
  "version": "1.0.12",
  "description": "Table ui component for react",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "father build",
    "build:watch": "father dev",
    "dev:publish": "yalc publish",
    "dev:watch": "nodemon --watch dist/es/ -e js,ts --debug -x pnpm build && yalc push",
    "docs:build": "dumi build",
    "husky:prepare": "husky install && dumi setup",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "doctor": "father doctor",
    "lint": "npm run lint:es",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "prepublishOnly": "father doctor && npm run build",
    "re-install": "pnpm run clean && rm pnpm-lock.yaml && pnpm install",
    "clean": "pnpm run clean:node_modules",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "classnames": "^2.5.1",
    "css-vars-ponyfill": "^2.4.3",
    "regenerator-runtime": "^0.13.9",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.6.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "xlsx": "^0.16.0",
    "antd": "^5.6.4",
    "@ant-design/cssinjs": "^1.18.2",
    "@ant-design/colors": "^7.0.2",
    "lodash-es": "^4.17.21",
    "@ctrl/tinycolor": "^3.6.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@rc-component/context": "^1.4.0",
    "rc-pagination": "~4.0.4",
    "ahooks": "^3.7.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/lodash-es": "^4.17.10",
    "@umijs/lint": "^4.0.0",
    "@ocloud/admin-eslint-config": "^1.0.0",
    "@ocloud/admin-prettier-config": "^1.0.0",
    "@ocloud/admin-stylelint-config": "^1.0.0",
    "@ocloud/admin-ts-config": "^1.0.0",
    "@ocloud/admin-commitlint-config": "^1.0.0",
    "@ocloud/admin-commitizen-config": "^1.0.0",
    "@ocloud/antd": "^1.2.0",
    "@ocloud/admin-hooks": "^1.2.0",
    "@ocloud/admin-context": "^1.2.0",
    "@ocloud/admin-config": "^1.2.0",
    "@ocloud/admin-utils": "^1.1.0",
    "@ant-design/icons": "^5.1.4",
    "rc-field-form": "~1.39.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "dumi": "^2.2.13",
    "eslint": "^8.31.0",
    "father": "^4.3.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "csstype": "^3.1.3",
    "react-activation": "^0.12.4"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.10",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "table",
    "rc-table",
    "o-rc-table",
    "react-table",
    "antd-table"
  ],
  "authors": [
    "Yooco"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pengdongliang/o-rc-table"
  },
  "license": "MIT"
}
